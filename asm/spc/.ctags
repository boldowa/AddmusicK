
--langmap=Asm:
--langdef=ASAR
--langmap=ASAR:.s.asm.inc.i.mac.tab
--regex-ASAR=/^([a-zA-Z_][a-zA-Z_0-9]+):/\1/l,label/
--regex-ASAR=/^\.([a-zA-Z_][a-zA-Z_0-9]+)/\1/l,label/
--regex-ASAR=/^\?([a-zA-Z_][a-zA-Z_0-9]+):/\1/l,label/
--regex-ASAR=/^\s*!([a-zA-Z_][a-zA-Z_0-9]+)\s*=\s*[^\s]+/\1/d,define/
--excmd=number
--exclude=Makefile

